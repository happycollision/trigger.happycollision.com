{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "lib": ["esnext", "dom", "WebWorker"],
    "baseUrl": ".",
    "paths": {
      "$data/*": ["src/data/*"],
      "$components/*": ["src/components/*"],
      "$layouts/*": ["src/layouts/*"],
      "$lib/*": ["src/lib/*"]
    }
  }
}
